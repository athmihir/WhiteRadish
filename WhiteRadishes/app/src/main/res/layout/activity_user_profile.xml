<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/offwhite"
    tools:context=".UserProfile">


    <ImageView
        android:id="@+id/transdesc1"
        android:layout_width="359dp"
        android:layout_height="422dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352"
        app:srcCompat="@drawable/trandesc1" />

    <ImageView
        android:id="@+id/transdesc2"
        android:layout_width="377dp"
        android:layout_height="252dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/transdesc1"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ar5" />


    <ImageView
        android:id="@+id/imgprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.081"
        app:srcCompat="@drawable/userimage" />

    <TextView
        android:id="@+id/profilename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name "
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/imgprofile"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/profilelocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location :"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilename"
        app:layout_constraintVertical_bias="0.044" />

    <TextView
        android:id="@+id/locval"
        android:layout_width="167dp"
        android:layout_height="21dp"
        android:text=""
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.75"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilename"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/contactval"
        android:layout_width="163dp"
        android:layout_height="27dp"
        android:text=""
        android:textSize="13sp"
        android:textAlignment="center"

        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilename"
        app:layout_constraintVertical_bias="0.215" />

    <TextView
        android:id="@+id/emailval"
        android:layout_width="169dp"
        android:layout_height="23dp"
        android:text=""
        android:textSize="13sp"
        android:textAlignment="center"

        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.78"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilename"
        app:layout_constraintVertical_bias="0.403" />

    <TextView
        android:id="@+id/addressval"
        android:layout_width="177dp"
        android:layout_height="27dp"
        android:text=""
        android:textSize="13sp"
        android:textAlignment="center"

        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.792"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilename"
        app:layout_constraintVertical_bias="0.61" />

    <TextView
        android:id="@+id/profilecontact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact :"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilelocation"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/profileemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email Id :"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profilecontact"
        app:layout_constraintVertical_bias="0.123" />

    <TextView
        android:id="@+id/profileaddr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address :"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc1"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toStartOf="@+id/transdesc1"
        app:layout_constraintTop_toBottomOf="@+id/profileemail"
        app:layout_constraintVertical_bias="0.211" />

    <ImageButton
        android:id="@+id/usercart"
        android:layout_width="164dp"
        android:layout_height="199dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc2"
        app:layout_constraintEnd_toEndOf="@+id/transdesc2"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="@+id/transdesc2"
        app:layout_constraintTop_toTopOf="@+id/transdesc2"
        app:layout_constraintVertical_bias="0.528"
        app:srcCompat="@drawable/usercart" />

    <ImageButton
        android:id="@+id/userorders"
        android:layout_width="162dp"
        android:layout_height="218dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/transdesc2"
        app:layout_constraintEnd_toEndOf="@+id/transdesc2"
        app:layout_constraintHorizontal_bias="0.233"
        app:layout_constraintStart_toEndOf="@+id/usercart"
        app:layout_constraintTop_toTopOf="@+id/transdesc2"
        app:layout_constraintVertical_bias="0.529"
        app:srcCompat="@drawable/userorders" />

    <ImageButton
        android:id="@+id/editbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="25dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/transdesc1"
        app:layout_constraintTop_toTopOf="@+id/transdesc1"
        app:srcCompat="@drawable/ic_edit" />


</androidx.constraintlayout.widget.ConstraintLayout>